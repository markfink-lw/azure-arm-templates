{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "resourceName": {
            "value": "your_cluster_name"
        },
        "location": {
            "value": "eastus"
        },
        "dnsPrefix": {
            "value": "your-cluster-dns"
        },
        "kubernetesVersion": {
            "value": "1.22.6"
        },
        "networkPlugin": {
            "value": "kubenet"
        },
        "enableRBAC": {
            "value": true
        },
        "enablePrivateCluster": {
            "value": false
        },
        "enableHttpApplicationRouting": {
            "value": false
        },
        "enableAzurePolicy": {
            "value": false
        },
        "enableSecretStoreCSIDriver": {
            "value": true
        },
        "vmssNodePool": {
            "value": true
        },
        "authorizedIPRanges": {
            "value": [
                "x.x.x.x/32"
            ]
        }
    }
}
