{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "networkInterfaceName": {
            "value": "your-nic-name"
        },
        "networkSecurityGroupName": {
            "value": "your-nsg"
        },
        "virtualNetworkName": {
            "value": "your-vnet"
        },
        "publicIpAddressName": {
            "value": "your-pip-name"
        },
        "virtualMachineName": {
            "value": "your-vmname"
        },
        "virtualMachineComputerName": {
            "value": "your-computer-name"
        },
        "osDiskType": {
            "value": "Standard_LRS"
        },
        "virtualMachineSize": {
            "value": "Standard_B1s"
        },
        "adminUsername": {
            "value": "root"
        },
        "sshKey": {
            "value": "your-key"
        },
        "laceworkEndpoint": {
            "value": "api.lacework.net"
        }
    }
}
